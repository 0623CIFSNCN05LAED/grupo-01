<section class="section-title__container">
    <h2>DESTACADOS</h2>
</section class="section">
<section class="section-product__container">
    <% womenProducts.forEach(product => { %>
        <div class="section-product">
            <a href="/products/<%= product.id %>">
                <img class="section-product__img" src="/images/producto-1-vitrina-1.jpg" alt="">
            </a>
            <h3><%= product.name %></h3>
            <p><%= product.price %></p>
        </div>
    <% }) %>

